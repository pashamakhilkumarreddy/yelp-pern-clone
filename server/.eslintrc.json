{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "no-console": [
      "error",
      {
        "allow": ["info", "warn", "error"]
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_table",
          "_dbReseach",
          "_initDbConnectionPool",
          "_hasTimestamps"
        ]
      }
    ]
  }
}
